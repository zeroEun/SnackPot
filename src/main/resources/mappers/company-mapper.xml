<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="companyMapper">

	<resultMap type="Company" id="companyResultSet">
		<id property="comCode" column="COM_CODE"/>
		<result property="comName" column="COM_NAME"/>
		<result property="comAddress" column="COM_ADDRESS"/>
		<result property="comAdmin" column="COM_ADMIN"/>
	</resultMap>
	
	<insert id="insertCompany" parameterType="Company">
		INSERT INTO COMPANY
		VALUES(#{comCode}, #{comName}, #{comAddress}, #{comAdmin})
	</insert>
	
</mapper>